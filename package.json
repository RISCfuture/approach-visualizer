    "@primeuix/styles": "^1.2.3",
    "@primeuix/themes": "^1.2.3",
    "babylonjs": "^8.22.2",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "primeflex": "^4.0.0",
    "primevue": "^4.3.7",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.18",
    "vue-i18n": "^11.1.11"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.2",
    "@playwright/test": "^1.54.2",
