    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "tailwindcss": "^4.1.14",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12"
